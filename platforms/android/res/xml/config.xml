<?xml version='1.0' encoding='utf-8'?>
<widget id="js.stage.orbit" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="FastContext">
        <param name="android-package" value="com.piqnt.fastcontext.FastContextPlugin" />
    </feature>
    <name>Orbit</name>
    <description>Orbit</description>
    <content src="index.html" />
